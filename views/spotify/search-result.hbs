<h1>Podcast Search Results</h1>


<div id="podcast-search-results">
  {{#each podcasts}}
    <div>
        <h2><a href="/spotify/details/{{this.id}}">{{this.name}}</a></h2>
        <img class="podcastImg" src="{{this.images.[1].url}}">
        <button type="submit">Add as favorite</button>
        
    </div>
  {{/each}}
</div>